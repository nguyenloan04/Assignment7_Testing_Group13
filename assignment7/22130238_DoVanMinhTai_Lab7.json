    "USERNOTIEmptyType01": {
        "configuration": {
            "device": {
                "orientation": "landscape",
                "deviceName": "Desktop"
            }
        },
        "type": "clickpath",
        "version": "1.0",
        "events": [
            {
                "description": "Loading of \"https://www.fahasa.com/customer/notification\"",
                "wait": {
                    "waitFor": "page_complete"
                },
                "target": {},
                "type": "navigate",
                "url": "https://www.fahasa.com/customer/notification"
            },
            {
                "description": "click",
                "wait": {
                    "waitFor": "page_complete"
                },
                "target": {
                    "locators": [
                        {
                            "type": "dom",
                            "value": "document.evaluate('//ul[@id=\\'notification-menu\\']/li[2]/a/div[2]', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                        }
                    ]
                },
                "type": "click",
                "button": 0
            },
            {
                "description": "waitForElementPresent",
                "wait": {
                    "waitFor": "validation",
                    "timeoutInMilliseconds": 60000,
                    "validation": {
                        "target": {
                            "locators": [
                                {
                                    "type": "dom",
                                    "value": "document.evaluate('//li[@data-id=\\'order\\' and contains(@class,\\'active\\')]//div[text()=\\'Đơn Hàng\\']', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                                }
                            ]
                        },
                        "type": "element_match",
                        "failIfFound": false,
                        "match": ""
                    }
                },
                "target": {},
                "type": "javascript",
                "javaScript": ""
            },
            {
                "description": "verifyText",
                "wait": {
                    "waitFor": "page_complete"
                },
                "target": {},
                "type": "javascript",
                "javaScript": "const elem = document.evaluate('//div[@id=\\'notification-body-empty\\']', document, null, XPathResult.ANY_TYPE, null).iterateNext();\n                        const textToMatch = elem.innerText;\n                        const matches = textToMatch === \"Không có thông báo.\";\n                        if(matches && false) {\n                            api.fail(\"Text Không có thông báo. was present.\");\n                        } else if(!matches && !false) {\n                            api.fail(\"Text Không có thông báo. was not present.\");\n                        }"
            }
        ]
    },
